{
    "root": true,
    "extends": [
        "eslint:recommended",
        "airbnb-base"
    ],
    "plugins": [
        "jest"
    ],
    "env": {
        "browser": true,
        "jest": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            4
        ]
    }
}